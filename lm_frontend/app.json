{
  "expo": {
    "name": "lm_frontend",
    "slug": "lm_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/LaundryMateIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#a7c9b1"
    },
    "icon": "./assets/images/LaundryMateIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.zamfiresq.lm-frontend"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#a7c9b1"
      },
      "package": "com.zamfiresq.lm_frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f1fe42ca-f30b-4fcf-bf30-0e4f1ccda282"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
